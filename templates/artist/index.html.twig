{% extends "base.html.twig" %}

{% block page_title %}
    <h1>Artistes</h1>
{% endblock %}

{% block page_content %}

{% for artist in artistList %}
    <h3>
        {{ artist.getNameA() }}
        <img src="{{ artist.getPictureA().getUrl() }}">

    </h3>
{% endfor %}

<style>
    img{
        max-width: 20%;
    }
</style>

{% endblock %}